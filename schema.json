{
    "name": "homelab",
    "displayName": "Homelab",
    "description": "Popular homelab applications for docker and kubernetes",
    "publisher": "UnstoppableMango",
    "keywords": [
        "category/cloud",
        "category/infrastructure",
        "kind/component",
        "pulumi",
        "kubernetes",
        "k8s",
        "docker",
        "homelab"
    ],
    "repository": "https://github.com/unmango/pulumi-homelab",
    "pluginDownloadURL": "",
    "types": {
        "homelab:k8s:Image": {
            "type": "object",
            "properties": {
                "registry": {
                    "type": "string",
                    "description": "Registry to use for the image. e.g. 'lcsr.io'"
                },
                "repository": {
                    "type": "string",
                    "description": "Repository to use for the image. e.g. 'linuxserver'"
                },
                "tag": {
                    "type": "string",
                    "description": "Tag to use for the image. e.g. 'latest'"
                }
            }
        },
        "homelab:k8s:Deployment": {
            "type": "object",
            "properties": {
                "strategy": {
                    "type": "object",
                    "$ref": "#/types/homelab:k8s:DeploymentStrategy",
                    "Description": "Deployment strategy to use."
                }
            }
        },
        "homelab:k8s:DeploymentStrategy": {
            "type": "object",
            "properties": {
                "type": {
                    "$ref": "#/types/homelab:k8s:DeploymentStrategyType",
                    "default": "Recreate",
                    "description": "Type of deployment strategy."
                }
            }
        },
        "homelab:k8s:DeploymentStrategyType": {
            "type": "string",
            "description": "Kubernetes deployment strategies.",
            "enum": [
                {
                    "value": "Recreate"
                },
                {
                    "value": "Rolling"
                }
            ]
        },
        "homelab:k8s:Service": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string",
                    "description": "Optional service name."
                },
                "type": {
                    "$ref": "#/types/homelab:k8s:ServiceType",
                    "description": "Type of service to deploy."
                }
            },
            "required": [
                "type"
            ]
        },
        "homelab:k8s:ServiceType": {
            "type": "string",
            "description": "Kubernetes service types.",
            "enum": [
                {
                    "value": "ClusterIP"
                },
                {
                    "value": "LoadBalancer"
                }
            ]
        },
        "homelab:k8s:PersistentVolumeClaimExistingClaim": {
            "type": "object",
            "description": "Arguments for specifying an existing persistent volume claim.",
            "properties": {
                "type": {
                    "type": "string",
                    "const": "existingClaim",
                    "description": "Type of persistent volume claim.\nExpected value is 'existingClaim'"
                },
                "existingClaim": {
                    "type": "string",
                    "description": "Existing volume claim to use."
                },
                "subPath": {
                    "type": "string",
                    "description": "Path within the volume to mount."
                }
            },
            "required": [
                "type",
                "existingClaim"
            ]
        },
        "homelab:k8s:PersistentVolumeClaimStorageClass": {
            "type": "object",
            "description": "Options for creating a persistent volume claim from a storage class.",
            "properties": {
                "type": {
                    "type": "string",
                    "const": "storageClass",
                    "description": "Type of persistent volume claim.\nExpected value is 'storageClass'"
                },
                "storageClass": {
                    "type": "string",
                    "description": "Storage class to use to provision the volume."
                },
                "subPath": {
                    "type": "string",
                    "description": "Path within the volume to mount."
                },
                "accessMode": {
                    "type": "string",
                    "description": "Access mode for the volume.",
                    "default": "ReadWriteOnce"
                },
                "size": {
                    "type": "string",
                    "description": "Size of the volume."
                }
            },
            "required": [
                "type",
                "storageClass"
            ]
        },
        "homelab:k8s/linuxserver:RadarrPersistence": {
            "type": "object",
            "description": "Radarr persistence options",
            "properties": {
                "config": {
                    "description": "Configuration persistence arguments",
                    "discriminator": {
                        "propertyName": "type",
                        "mapping": {
                            "existingClaim": "#/types/homelab:k8s:PersistentVolumeClaimExistingClaim",
                            "storageClass": "#/types/homelab:k8s:PersistentVolumeClaimStorageClass"
                        }
                    },
                    "oneOf": [
                        {
                            "type": "object",
                            "$ref": "#/types/homelab:k8s:PersistentVolumeClaimExistingClaim"
                        },
                        {
                            "type": "object",
                            "$ref": "#/types/homelab:k8s:PersistentVolumeClaimStorageClass"
                        }
                    ],
                    "plain": true
                },
                "downloads": {
                    "description": "Downloads persistence arguments",
                    "discriminator": {
                        "propertyName": "type",
                        "mapping": {
                            "existingClaim": "#/types/homelab:k8s:PersistentVolumeClaimExistingClaim",
                            "storageClass": "#/types/homelab:k8s:PersistentVolumeClaimStorageClass"
                        }
                    },
                    "oneOf": [
                        {
                            "type": "object",
                            "$ref": "#/types/homelab:k8s:PersistentVolumeClaimExistingClaim"
                        },
                        {
                            "type": "object",
                            "$ref": "#/types/homelab:k8s:PersistentVolumeClaimStorageClass"
                        }
                    ],
                    "plain": true
                },
                "movies": {
                    "description": "Movies persistence arguments",
                    "discriminator": {
                        "propertyName": "type",
                        "mapping": {
                            "existingClaim": "#/types/homelab:k8s:PersistentVolumeClaimExistingClaim",
                            "storageClass": "#/types/homelab:k8s:PersistentVolumeClaimStorageClass"
                        }
                    },
                    "oneOf": [
                        {
                            "type": "object",
                            "$ref": "#/types/homelab:k8s:PersistentVolumeClaimExistingClaim"
                        },
                        {
                            "type": "object",
                            "$ref": "#/types/homelab:k8s:PersistentVolumeClaimStorageClass"
                        }
                    ],
                    "plain": true
                }
            }
        }
    },
    "resources": {
        "homelab:index:StaticPage": {
            "isComponent": true,
            "inputProperties": {
                "indexContent": {
                    "type": "string",
                    "description": "The HTML content for index.html."
                }
            },
            "requiredInputs": [
                "indexContent"
            ],
            "properties": {
                "bucket": {
                    "$ref": "/aws/v4.0.0/schema.json#/resources/aws:s3%2Fbucket:Bucket",
                    "description": "The bucket resource."
                },
                "websiteUrl": {
                    "type": "string",
                    "description": "The website URL."
                }
            },
            "required": [
                "bucket",
                "websiteUrl"
            ]
        },
        "homelab:k8s/linuxserver:Radarr": {
            "isComponent": true,
            "inputProperties": {
                "name": {
                    "type": "string",
                    "description": "Optional name override."
                },
                "namespace": {
                    "type": "string",
                    "description": "Namespace to provision resources in."
                },
                "image": {
                    "description": "Optional custom image to use.",
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "$ref": "#/types/homelab:k8s:Image"
                        }
                    ]
                },
                "deployment": {
                    "type": "object",
                    "$ref": "#/types/homelab:k8s:Deployment",
                    "description": "Arguments for the kubernetes deployment."
                },
                "service": {
                    "$ref": "#/types/homelab:k8s:Service",
                    "description": "Arguments for the kubernetes service."
                },
                "persistence": {
                    "$ref": "#/types/homelab:k8s/linuxserver:RadarrPersistence",
                    "description": "Options for persistence.",
                    "plain": true
                },
                "puid": {
                    "type": "string",
                    "description": "User ID to run as."
                },
                "pgid": {
                    "type": "string",
                    "description": "Group ID to run as."
                },
                "tz": {
                    "type": "string",
                    "description": "Timezone to use. e.g. Europe/London",
                    "default": "Europe/London"
                }
            },
            "properties": {
                "deployment": {
                    "$ref": "/kubernetes/v3.12.1/schema.json#/resources/kubernetes:apps%2Fv1:Deployment",
                    "description": "Radarr deployment."
                },
                "port": {
                    "type": "number",
                    "description": "Radarr service port."
                },
                "service": {
                    "$ref": "/kubernetes/v3.12.1/schema.json#/resources/kubernetes:core%2Fv1:Service",
                    "description": "Radarr service."
                },
                "serviceName": {
                    "type": "string",
                    "description": "Radarr service name."
                }
            },
            "required": [
                "deployment",
                "port",
                "service",
                "serviceName"
            ]
        }
    },
    "language": {
        "csharp": {
            "dictionaryConstructors": true,
            "namespaces": {
                "": "Provider",
                "k8s": "K8s",
                "k8s/linuxserver": "K8s.LinuxServer"
            },
            "packageReferences": {
                "Pulumi": "3.*",
                "Pulumi.Aws": "4.*",
                "Pulumi.Docker": "3.*",
                "Pulumi.Kubernetes": "3.*"
            }
        },
        "go": {
            "generateResourceContainerTypes": true,
            "importBasePath": "github.com/unmango/pulumi-homelab/sdk/go/homelab"
        },
        "nodejs": {
            "packageName": "@unmango/pulumi-homelab",
            "dependencies": {
                "@pulumi/aws": "^4.0.0",
                "@pulumi/docker": "^3.1.0",
                "@pulumi/kubernetes": "^3.12.1"
            },
            "devDependencies": {
                "typescript": "^4.5.4"
            }
        },
        "python": {
            "requires": {
                "pulumi": ">=3.0.0,<4.0.0",
                "pulumi-aws": ">=4.0.0,<5.0.0",
                "pulumi-docker": ">=3.0.0,<4.0.0",
                "pulumi-kubernetes": ">=3.0.0,<4.0.0"
            }
        }
    }
}
